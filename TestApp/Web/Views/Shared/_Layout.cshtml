    <script type="text/javascript">
        $(function () {
            $("#flash-messages").flashMessage();
        });

        function mostrarMensaje(titulo, texto) {
            $('#idTituloMensaje').html(titulo);
            $('#idTextoMensaje').html(texto);
            $('#idModalTexto').modal('show');
        }

        $(document).ready(function () {
            $(".select2").select2({ allowClear: true })
               .on('change', function () {
                   $(this).closest('form').validate().element($(this));
               }).addClass('width-100 input-sm no-padding');

            bootbox.setDefaults({
                locale: "es",
                backdrop: true
            });
            
            $(".onlynumber").numericInput({ allowFloat: false, allowNegative: false });
            $(".onlynumberFloat").numericInput({ allowNegative: false, allowFloat: true });
        });

        function DisableBackButtonAllBrowsers() {
            window.history.forward();
        };

        function DisableBack() {
            DisableBackButtonAllBrowsers();
            window.onload = DisableBackButtonAllBrowsers;
            window.onpageshow = function (evts) { if (evts.persisted) DisableBackButtonAllBrowsers(); };
            window.onunload = function () { void (0) };
        }

    </script>
    
    <title> Test App </title>
</head>

